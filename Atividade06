 /*Ler dois valores para v치riavel A e B, e efetuar as trocas dos valores de forma que a vari치vel A passe a possuir o valor B e a v치riavel
             B passe a possuir o valor da vari치vel A. Apresentar os valores trocados */

            double a, b, c;
            Console.Write("Informe o valor de A: ");
            a = double.Parse(Console.ReadLine());

            Console.Write("Informe o valor de B: ");
            b = double.Parse(Console.ReadLine());

            c = a;
            a = b;
            b = c;

            Console.WriteLine($" \nValor invertido de A:{a} ");
            Console.WriteLine($"\nValor invertido  de B:{b} ");

            Console.ReadKey();